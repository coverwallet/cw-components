{
  "name": "cw-components",
  "version": "2.31.1",
  "description": "CoverWallet components",
  "main": "./build/lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/coverwallet/cw-components.git"
  },
  "homepage": "https://github.com/coverwallet/cw-components",
  "bugs": "https://github.com/coverwallet/cw-components/issues",
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "start": "webpack-dev-server --inline --content-base examples/ --port 8002",
    "test": "jest",
    "build": "babel ./lib -d build/lib --ignore './lib/components/__tests__'",
    "build-watch": "npm run styleguide-fonts & babel -w -d build/lib ./lib",
    "copy-fonts": "cp -R node_modules/cw-styleguide/fonts examples/basic",
    "copy-images": "cp -R node_modules/cw-styleguide/images examples/basic",
    "styleguide-fonts": "npm run copy-fonts & npm run copy-images",
    "gh-pages": "./scripts/gh-pages.sh",
    "bower": "./scripts/bower.sh",
    "prepublish": "npm run build",
    "sync": "browser-sync start --proxy 'http://localhost:8002/basic/'"
  },
  "authors": [
    "CoverWallet"
  ],
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.6.5",
    "babel-eslint": "^6.1.2",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "browser-sync": "^2.26.3",
    "css-loader": "^0.23.1",
    "cw-styleguide": "git+https://github.com/coverwallet/styleguide.git#2.8.10",
    "enzyme": "^2.8.0",
    "eslint": "^2.9.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.7.0",
    "eslint-plugin-jsx-a11y": "^1.2.1",
    "eslint-plugin-react": "^5.2.2",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.10.0",
    "jest": "^19.0.2",
    "node-sass": "^3.4.2",
    "object-assign": "^4.0.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-test-renderer": "^15.4.2",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  },
  "peerDependencies": {
    "react": "^0.14.3 || ^15.4.2",
    "react-dom": "^0.14.3 || ^15.4.2"
  },
  "tags": [
    "React",
    "Component",
    ""
  ],
  "keywords": [
    "React",
    "Component",
    ""
  ],
  "dependencies": {
    "classnames": "^2.2.3",
    "lodash": "^4.15.0",
    "prop-types": "^15.5.8",
    "react-highlight-words": "^0.11.0",
    "smoothscroll": "^0.2.2"
  }
}
